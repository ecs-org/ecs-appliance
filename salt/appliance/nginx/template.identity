server_name ##domains##
ssl_certificate /etc/nginx/app/server.cert.pem;
ssl_certificate_key /etc/nginx/app/server.key.pem;
# Diffie-Hellman parameter for DHE ciphersuites, recommended 2048 bits
ssl_dhparam /etc/nginx/app/dhparam.pem;
# client certificate setup
ssl_client_certificate /etc/nginx/app/ca.cert.pem;
ssl_crl /etc/nginx/app/crl.pem;
ssl_verify_client optional;
